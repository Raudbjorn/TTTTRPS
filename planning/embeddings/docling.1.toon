# ═══════════════════════════════════════════════════════════════════════════════
# PYTHON API
# ═══════════════════════════════════════════════════════════════════════════════

api:

  basic_usage: |
    from docling.document_converter import DocumentConverter

    converter = DocumentConverter()
    result = converter.convert("document.pdf")
    print(result.document.export_to_markdown())

  convert_url: |
    from docling.document_converter import DocumentConverter

    converter = DocumentConverter()
    result = converter.convert("https://arxiv.org/pdf/2408.09869")
    result.document.save_as_json("output.json")

  batch_conversion: |
    from docling.document_converter import DocumentConverter
    from pathlib import Path

    converter = DocumentConverter()
    results = converter.convert_all([
        Path("doc1.pdf"),
        Path("doc2.pdf"),
        "https://example.com/doc3.pdf"
    ])
    for result in results:
        print(f"{result.input.file}: {result.status}")

  custom_pipeline_options: |
    from docling.document_converter import DocumentConverter, PdfFormatOption
    from docling.datamodel.pipeline_options import PdfPipelineOptions
    from docling.datamodel.base_models import InputFormat

    pipeline_options = PdfPipelineOptions(
        do_ocr=True,
        do_table_structure=True,
        do_code_enrichment=True,
        do_formula_enrichment=True,
        generate_page_images=True,
    )

    converter = DocumentConverter(
        format_options={
            InputFormat.PDF: PdfFormatOption(pipeline_options=pipeline_options)
        }
    )
    result = converter.convert("document.pdf")

  custom_ocr: |
    from docling.document_converter import DocumentConverter, PdfFormatOption
    from docling.datamodel.pipeline_options import PdfPipelineOptions, RapidOcrOptions
    from docling.datamodel.base_models import InputFormat

    pipeline_options = PdfPipelineOptions(
        do_ocr=True,
        ocr_options=RapidOcrOptions(lang=["english", "chinese"])
    )

    converter = DocumentConverter(
        format_options={
            InputFormat.PDF: PdfFormatOption(pipeline_options=pipeline_options)
        }
    )

  vlm_pipeline: |
    from docling.document_converter import DocumentConverter, PdfFormatOption
    from docling.datamodel.pipeline_options import VlmPipelineOptions
    from docling.datamodel import vlm_model_specs
    from docling.datamodel.base_models import InputFormat
    from docling.pipeline.vlm_pipeline import VlmPipeline

    pipeline_options = VlmPipelineOptions(
        vlm_options=vlm_model_specs.GRANITEDOCLING_TRANSFORMERS
    )

    converter = DocumentConverter(
        format_options={
            InputFormat.PDF: PdfFormatOption(
                pipeline_cls=VlmPipeline,
                pipeline_options=pipeline_options
            )
        }
    )

  asr_pipeline: |
    from docling.document_converter import DocumentConverter, AudioFormatOption
    from docling.datamodel.pipeline_options import AsrPipelineOptions
    from docling.datamodel.asr_model_specs import WHISPER_MEDIUM
    from docling.datamodel.base_models import InputFormat
    from docling.pipeline.asr_pipeline import AsrPipeline

    pipeline_options = AsrPipelineOptions(asr_options=WHISPER_MEDIUM)

    converter = DocumentConverter(
        format_options={
            InputFormat.AUDIO: AudioFormatOption(
                pipeline_cls=AsrPipeline,
                pipeline_options=pipeline_options
            )
        }
    )
    result = converter.convert("audio.mp3")

  export_options: |
    # Markdown
    result.document.export_to_markdown()
    result.document.save_as_markdown("output.md", image_mode=ImageRefMode.EMBEDDED)

    # JSON
    result.document.save_as_json("output.json", image_mode=ImageRefMode.REFERENCED)

    # HTML
    result.document.save_as_html("output.html", split_page_view=True)

    # Plain text
    result.document.export_to_markdown(strict_text=True)

    # DocTags (for LLM training)
    result.document.save_as_doctags("output.doctags")

  accelerator_options: |
    from docling.datamodel.accelerator_options import AcceleratorOptions, AcceleratorDevice
    from docling.datamodel.pipeline_options import PdfPipelineOptions

    pipeline_options = PdfPipelineOptions(
        accelerator_options=AcceleratorOptions(
            num_threads=4,
            device=AcceleratorDevice.CUDA  # or CPU, MPS, AUTO
        )
    )

  page_range: |
    from docling.datamodel.settings import PageRange

    # Convert specific pages
    result = converter.convert(
        "document.pdf",
        page_range=PageRange(start=1, end=10)
    )

  error_handling: |
    from docling.datamodel.base_models import ConversionStatus

    result = converter.convert("document.pdf", raises_on_error=False)

    if result.status == ConversionStatus.SUCCESS:
        print(result.document.export_to_markdown())
    elif result.status == ConversionStatus.PARTIAL_SUCCESS:
        print("Partial conversion:", result.errors)
    else:
        print("Failed:", result.errors)

# ═══════════════════════════════════════════════════════════════════════════════
# CLI
# ═══════════════════════════════════════════════════════════════════════════════

cli:
  command: docling
  install: pip install docling

  basic_usage: |
    # Convert PDF to Markdown
    docling document.pdf

    # Convert URL
    docling https://arxiv.org/pdf/2408.09869

    # Convert directory
    docling ./documents/

  output_formats: |
    docling document.pdf --to md          # Markdown (default)
    docling document.pdf --to json        # JSON
    docling document.pdf --to yaml        # YAML
    docling document.pdf --to html        # HTML
    docling document.pdf --to text        # Plain text
    docling document.pdf --to doctags     # DocTags
    docling document.pdf --to md --to json  # Multiple formats

  vlm_mode: |
    # Use VLM pipeline with GraniteDocling
    docling --pipeline vlm --vlm-model granite_docling document.pdf

    # Other VLM models
    docling --pipeline vlm --vlm-model smoldocling document.pdf
    docling --pipeline vlm --vlm-model granite_vision document.pdf

  audio_transcription: |
    docling audio.mp3 --asr-model whisper_tiny
    docling audio.wav --asr-model whisper_medium
    docling video.mp4 --asr-model whisper_large

  ocr_options: |
    docling document.pdf --ocr                     # Enable OCR (default)
    docling document.pdf --no-ocr                  # Disable OCR
    docling document.pdf --force-ocr               # Force full-page OCR
    docling document.pdf --ocr-engine rapidocr     # Specific engine
    docling document.pdf --ocr-engine tesseract --ocr-lang deu,eng

  table_options: |
    docling document.pdf --tables                  # Enable tables (default)
    docling document.pdf --no-tables               # Disable tables
    docling document.pdf --table-mode accurate     # Accurate mode
    docling document.pdf --table-mode fast         # Fast mode

  enrichment: |
    docling document.pdf --enrich-code             # Code block enrichment
    docling document.pdf --enrich-formula          # Formula to LaTeX
    docling document.pdf --enrich-picture-classes  # Picture classification
    docling document.pdf --enrich-picture-description  # Picture descriptions

  image_handling: |
    docling document.pdf --image-export-mode embedded   # Base64 inline
    docling document.pdf --image-export-mode referenced # Separate files
    docling document.pdf --image-export-mode placeholder # Position only

  performance: |
    docling document.pdf --num-threads 8           # Thread count
    docling document.pdf --device cuda             # GPU acceleration
    docling document.pdf --device mps              # Apple Metal
    docling document.pdf --document-timeout 60     # Timeout in seconds

  other_options: |
    docling document.pdf --output ./results        # Output directory
    docling document.pdf -v                        # Verbose (info)
    docling document.pdf -vv                       # Debug logging
    docling document.pdf --profiling               # Show timing stats
    docling document.pdf --abort-on-error          # Stop on first error
    docling document.pdf --pdf-password secret     # Protected PDF

