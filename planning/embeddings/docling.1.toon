# ═══════════════════════════════════════════════════════════════════════════════
# INSTALLATION
# ═══════════════════════════════════════════════════════════════════════════════

installation:
  basic: pip install docling

  with_ocr: |
    pip install docling[ocr]          # All OCR engines
    pip install docling[tesseract]    # Tesseract only
    pip install docling[rapidocr]     # RapidOCR only
    pip install docling[easyocr]      # EasyOCR only

  with_vlm: |
    pip install docling[vlm]          # VLM models
    pip install mlx-vlm               # Apple MLX acceleration

  with_asr: |
    pip install docling[asr]          # ASR models

  full: pip install docling[all]

  system_deps: |
    # Ubuntu/Debian
    apt-get install tesseract-ocr libtesseract-dev

    # macOS
    brew install tesseract

    # Poppler for PDF (optional)
    apt-get install poppler-utils  # Linux
    brew install poppler           # macOS

# ═══════════════════════════════════════════════════════════════════════════════
# BEST PRACTICES
# ═══════════════════════════════════════════════════════════════════════════════

best_practices:

  performance:
    - Use RapidOCR for best speed/quality balance
    - Set appropriate num_threads for your CPU
    - Use GPU acceleration (CUDA/MPS) when available
    - Set document_timeout for batch processing
    - Disable unused enrichment options

  quality:
    - Use accurate table mode for complex tables
    - Enable OCR for scanned documents
    - Use VLM pipeline for challenging layouts
    - Generate page images for verification

  memory:
    - Process large documents in batches
    - Clear page images after processing
    - Use page_range for partial conversion
    - Set appropriate batch sizes

  formats:
    - Use JSON for programmatic access
    - Use Markdown for LLM consumption
    - Use DocTags for model training
    - Use HTML for visualization

