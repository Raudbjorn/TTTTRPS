architecture:
  flow: DocumentConverter → InputDocument → Backend → Pipeline → ConversionResult
  pipeline_stages[3]: _build_document(),_assemble_document(),_enrich_document()
  storage: LMDB for caching
  ml_models: Heron (layout) TableFormer (tables) VLMs (understanding) Whisper (ASR)

usage_patterns:
  basic_pdf: |
    from docling.document_converter import DocumentConverter
    converter = DocumentConverter()
    result = converter.convert("document.pdf")
    print(result.document.export_to_markdown())

  batch_with_errors: |
    for result in converter.convert_all(docs, raises_on_error=False):
        if result.status == ConversionStatus.SUCCESS:
            result.document.save_as_markdown(f"{result.input.file.stem}.md")

  advanced_pdf: |
    options = PdfPipelineOptions(do_ocr=True, do_table_structure=True)
    converter = DocumentConverter(format_options={InputFormat.PDF: PdfFormatOption(pipeline_options=options)})
    result = converter.convert("scanned.pdf")

  extract_tables: |
    for item, depth in doc.iterate_items():
        if isinstance(item, TableItem):
            for row in item.table.rows:
                print([cell.text for cell in row.cells])

error_handling:
  error_item: component_type module_name error_message
  component_types: BACKEND PIPELINE OCR TABLE_STRUCTURE LAYOUT VLM ASR
  check_status: if result.status == ConversionStatus.FAILURE: print(result.errors)

performance:
  batch_processing: convert_all() for multiple documents
  threading: ThreadedStandardPdfPipeline for parallel PDF
  caching: Pipeline caching via initialize_pipeline()
  pre_download: docling-tools models download
  memory: page_batch_size controls memory usage

best_practices[8]:
  - Use convert_all() for batch processing
  - Pre-download models with docling-tools models download
  - Set raises_on_error=False for batch error handling
  - Use NDJSON output for streaming large documents
  - Configure page_batch_size for memory-constrained environments
  - Use ThreadedStandardPdfPipeline for concurrent PDF processing
  - Enable only needed enrichments (OCR tables formulas)
  - Use ImageRefMode.PLACEHOLDER for smaller output files
