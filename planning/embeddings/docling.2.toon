env_vars[8]: DOCLING_ARTIFACTS_PATH,DOCLING_PERF_DOC_BATCH_SIZE,DOCLING_PERF_DOC_BATCH_CONCURRENCY,DOCLING_PERF_PAGE_BATCH_SIZE,DOCLING_PERF_ELEMENTS_BATCH_SIZE,DOCLING_DEBUG_VISUALIZE_LAYOUT,DOCLING_DEBUG_OUTPUT_PATH,DOCLING_CACHE_DIR

