meta:
  name: Docling Document Processing SDK
  version: 2.67.0
  description: Parse diverse document formats into unified DoclingDocument representation
  license: MIT
  python: 3.9 - 3.14
  organization: LF AI & Data Foundation
  repo: https://github.com/docling-project/docling
  docs: https://docling-project.github.io/docling/

overview:
  what: Open-source document processing SDK for parsing PDFs, Office docs, HTML, images, and audio into structured format
  by: IBM Research
  core_features[12]: Multi-format parsing (PDF DOCX PPTX XLSX HTML MD images audio),Advanced PDF understanding (layout tables reading-order),Local execution (air-gapped environments),OCR support (RapidOCR EasyOCR Tesseract),Vision Language Models (GraniteDocling SmolDocling),Table structure extraction (TableFormer),Code and formula recognition,Picture classification and description,Chunking for RAG workflows,LangChain/LlamaIndex/Haystack integrations,MCP server support,Structured information extraction

input_formats[14]: PDF,DOCX,DOCM,DOTX,PPTX,POTX,PPSX,XLSX,XLSM,HTML,XHTML,Markdown,AsciiDoc,CSV,PNG,JPEG,TIFF,BMP,WEBP,USPTO XML,JATS XML,WebVTT,WAV,MP3,M4A,AAC,OGG,FLAC,MP4,AVI,MOV

output_formats[7]: Markdown,HTML,HTML (split-page),JSON (lossless),YAML,Plain Text,DocTags

installation:
  basic: pip install docling
  ocr_easyocr: pip install docling[easyocr]
  ocr_tesseract: pip install docling[tesseract]
  ocr_mac: pip install docling[ocrmac]
  vlm: pip install docling[vlm]
  asr: pip install docling[asr]
  all: pip install docling[easyocr,tesseract,vlm,asr]
  models: docling-tools models download

api_basic:
  import: from docling.document_converter import DocumentConverter
  init: converter = DocumentConverter()
  convert_single: result = converter.convert("document.pdf")
  convert_batch: for result in converter.convert_all(["a.pdf", "b.docx"]): ...
  convert_string: result = converter.convert_string(content, InputFormat.MD)
  get_document: doc = result.document
  export_markdown: doc.export_to_markdown()
  export_dict: doc.export_to_dict()
  save_markdown: doc.save_as_markdown("output.md", image_mode=ImageRefMode.EMBEDDED)
  save_html: doc.save_as_html("output.html", split_page_view=False)
  save_json: doc.save_as_json("output.json")

converter_methods[4]{method,signature,description}:
  convert,"(source, headers=None, raises_on_error=True, max_num_pages=maxsize, max_file_size=maxsize, page_range=DEFAULT) -> ConversionResult",Single document conversion
  convert_all,"(source: Iterable, ...) -> Iterator[ConversionResult]",Batch conversion iterator
  convert_string,"(content: str, format: InputFormat, name=None) -> ConversionResult",Convert MD or HTML string
  initialize_pipeline,"(format: InputFormat) -> None",Pre-initialize pipeline for format

conversion_result:
  input: InputDocument
  status: ConversionStatus (SUCCESS FAILURE PARTIAL_SUCCESS PENDING SKIPPED)
  errors: list[ErrorItem]
  pages: list[Page]
  timings: dict[str ProfilingItem]
  confidence: ConfidenceReport
  document: DoclingDocument

docling_document:
  name: str
  main: DocItemList
  header: Optional[DocItemList]
  footer: Optional[DocItemList]
  body: Optional[DocItemList]
  methods[7]: iterate_items(),export_to_dict(),export_to_markdown(),export_to_text(),save_as_json(),save_as_html(),save_as_yaml(),save_as_doctags()

doc_item_types[15]: TextItem,SectionHeaderItem,ListItem,TableItem,PictureItem,FormItem,FormulaItem,CodeItem,PageFooterItem,PageHeaderItem,FootnoteItem,CaptionItem,CheckboxItem,KeyValueRegionItem,TableCell

pipeline_options:
  pdf:
    do_table_structure: true
    do_ocr: false
    do_code_enrichment: false
    do_formula_enrichment: false
    generate_page_images: false
    images_scale: 1.0
    ocr_options: OcrAutoOptions()
    table_structure_options: TableStructureOptions(mode=ACCURATE)
    layout_options: LayoutOptions(model_spec=DOCLING_LAYOUT_HERON)

pipeline_options_example: |
  from docling.datamodel.pipeline_options import PdfPipelineOptions, TableStructureOptions, TableFormerMode, RapidOcrOptions
  from docling.document_converter import DocumentConverter, PdfFormatOption
  from docling.datamodel.base_models import InputFormat

  options = PdfPipelineOptions(
      do_table_structure=True,
      do_ocr=True,
      ocr_options=RapidOcrOptions(lang=["english"]),
      table_structure_options=TableStructureOptions(mode=TableFormerMode.ACCURATE)
  )
  converter = DocumentConverter(format_options={InputFormat.PDF: PdfFormatOption(pipeline_options=options)})

ocr_engines[5]{engine,install,languages,notes}:
  RapidOCR,built-in,100+,Default ONNX-based fast OCR
  EasyOCR,docling[easyocr],80+,PyTorch GPU-friendly
  Tesseract CLI,docling[tesseract],40+,Traditional OCR engine
  tesserocr,docling[tesserocr],40+,Direct Python binding
  OCR Mac,docling[ocrmac],built-in,macOS Vision API (Apple Silicon)

ocr_options:
  OcrAutoOptions: lang=[] force_full_page_ocr=False bitmap_area_threshold=0.05
  RapidOcrOptions: lang=["english"] backend="onnxruntime" text_score=0.5
  EasyOcrOptions: lang=["en"] gpu=True
  TesseractCliOcrOptions: lang=["eng"] psm=3 tesseract_cmd="tesseract"
  OcrMacOptions: lang=["en-US"] recognition_level="accurate"

table_extraction:
  model: TableFormer
  modes[2]: fast,accurate
  options: TableStructureOptions(mode=TableFormerMode.ACCURATE, do_cell_matching=True)
  output: TableItem with TableData containing rows cells and bounding boxes

layout_models[6]{name,description}:
  DOCLING_LAYOUT_HERON,Default faster model
  DOCLING_LAYOUT_HERON_101,Heron v1.0.1
  DOCLING_LAYOUT_V2,Legacy layout model
  DOCLING_LAYOUT_EGRET_MEDIUM,More powerful medium
  DOCLING_LAYOUT_EGRET_LARGE,More powerful large
  DOCLING_LAYOUT_EGRET_XLARGE,Most powerful

vlm_models[8]{name,params,notes}:
  GRANITEDOCLING_TRANSFORMERS,258M,IBM Granite DocTags output
  GRANITEDOCLING_MLX,258M,Apple Silicon optimized
  GRANITEDOCLING_VLLM,258M,vLLM API-based
  GRANITEDOCLING_OLLAMA,258M,Ollama API-based
  SMOLDOCLING_TRANSFORMERS,256M,Compact efficient
  SMOLDOCLING_MLX,256M,Apple Silicon
  SMOLVLM256_TRANSFORMERS,256M,Multi-purpose VLM
  NU_EXTRACT_2B_TRANSFORMERS,2B,Extraction focused

asr_models[7]: WHISPER_TINY,WHISPER_BASE,WHISPER_SMALL,WHISPER_MEDIUM,WHISPER_LARGE,WHISPER_TURBO,MLX variants

pipelines[6]{name,use_case}:
  SimplePipeline,Declarative backends (DOCX HTML MD)
  StandardPdfPipeline,Production PDF with layout OCR tables
