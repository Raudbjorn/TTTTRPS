image_modes[3]: EMBEDDED (base64),REFERENCED (separate PNG files),PLACEHOLDER ([Image] markers)

cli:
  basic: docling document.pdf
  output_format: docling --to md,json file.pdf
  with_ocr: docling --ocr --ocr-engine easyocr --ocr-lang eng,deu input.pdf
  force_ocr: docling --force-ocr input.pdf
  with_tables: docling --tables --table-mode accurate document.pdf
  with_vlm: docling --pipeline vlm --vlm-model granite_docling document.pdf
  with_asr: docling --pipeline asr --asr-model whisper-base audio.mp3
  output_dir: docling -o ./output/ document.pdf
  code_formulas: docling --enrich-code --enrich-formula technical.pdf
  picture_desc: docling --enrich-picture-desc document.pdf
  show_layout: docling --show-layout document.pdf
  version: docling --version

cli_options[25]{option,description}:
  --from FORMAT,Input format(s) to accept
  --to FORMAT,Output format(s) (md json html txt yaml doctags html_split_page)
  --pipeline PIPELINE,Processing pipeline (standard vlm legacy asr)
  --vlm-model MODEL,VLM model name
  --asr-model MODEL,ASR model name
  --ocr,Enable OCR for scanned documents
  --force-ocr,Force full-page OCR (replace text)
  --ocr-engine ENGINE,OCR engine (auto easyocr tesseract rapidocr ocrmac)
  --ocr-lang LANGS,Languages for OCR (comma-separated)
  --tables,Extract table structure
  --table-mode MODE,Table extraction mode (fast accurate)
  --enrich-code,Enable code block OCR
  --enrich-formula,Enable LaTeX formula extraction
  --enrich-picture-classes,Enable picture classification
  --enrich-picture-desc,Enable VLM picture description
  --pdf-backend BACKEND,PDF parser (pypdfium2 dlparse_v1 dlparse_v2 dlparse_v4)
  --pdf-password PASSWORD,Password for protected PDFs
  --image-export-mode MODE,Image handling (embedded referenced placeholder)
  --show-layout,Visualize layout bounding boxes
  --artifacts-path PATH,Pre-downloaded model artifacts location
  --enable-remote-services,Allow external API calls
  --allow-external-plugins,Load external third-party plugins
  -o --output-dir DIR,Output directory
  --images-scale SCALE,Scale factor for page images (default 1.0)
  --version,Show version info

